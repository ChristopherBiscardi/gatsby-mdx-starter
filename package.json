{
  "name": "gatsby-starter-pack",
  "description": "Gatsby Starter Pack",
  "version": "1.0.0",
  "author": "John Munson <johncmunson91@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^0.15.2",
    "@mdx-js/tag": "^0.15.0",
    "gatsby": "next",
    "gatsby-mdx": "^0.1.3",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-react-helmet": "next",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-live": "^1.11.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write '{,!(node_modules|.cache|public)/**/}*.{js,json,css,md}'",
    "lint": "./node_modules/.bin/eslint '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.0.0-rc.14",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{,!(node_modules|.cache|public)/**/}*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
